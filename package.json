{
  "name": "koncert",
  "description": "A progressive web app displaying Danish concerts.",
  "version": "1.0.0",
  "author": "Christoffer Kjeldgaard Petersen",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run test && npm run compile",
    "build:parts": "npm run sass && npm run compile",
    "clean": "rimraf dist && rimraf coverage",
    "codecov": "codecov -f coverage/json/coverage-final.json",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "cypress:open": "./node_modules/cypress/bin/cypress open",
    "dev": "webpack-dev-server --hot --inline",
    "e2e": "./node_modules/cypress/bin/cypress run --record --key 7d4c3ee7-b775-4203-9727-43328ecbfbf0",
    "lint": "tslint 'src/**/*.ts'",
    "mock": "webpack-dev-server --hot --inline --define process.env.NODE_ENV=\"mock\" --quiet",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "firebase": "^4.6.0",
    "moment": "2.19.1",
    "surface": "github:mildrenben/surface",
    "vue": "^2.5.2",
    "vue-class-component": "~6.0.0",
    "vue-on-click-outside": "^1.0.3",
    "vue-property-decorator": "~6.0.0",
    "vue-router": "~3.0.1",
    "vue-template-compiler": "^2.5.2"
  },
  "devDependencies": {
    "@types/chai-dom": "0.0.5",
    "@types/mocha": "~2.2.43",
    "@types/node": "~8.0.47",
    "@types/sinon": "~2.3.7",
    "autoprefixer": "^7.1.6",
    "avoriaz": "^6.0.1",
    "awesome-typescript-loader": "~3.2.3",
    "chai": "~4.1.2",
    "chai-dom": "^1.5.0",
    "codecov": "^3.0.0",
    "compression-webpack-plugin": "~1.0.1",
    "copy-webpack-plugin": "~4.2.0",
    "cross-env": "~5.1.0",
    "css-loader": "^0.28.7",
    "cypress": "^1.0.2",
    "es6-promise": "~4.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "~3.0.0",
    "json-loader": "~0.5.7",
    "karma": "~1.7.1",
    "karma-chai": "~0.1.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.5",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.1",
    "karma-webpack": "~2.0.5",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~4.0.1",
    "ncp": "~2.0.0",
    "node-sass": "~4.5.3",
    "opn-cli": "~3.1.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.5",
    "rimraf": "~2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "~4.0.2",
    "style-loader": "^0.19.0",
    "sw-precache-webpack-dev-plugin": "^0.2.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "tslint": "~5.8.0",
    "tslint-loader": "~3.5.3",
    "typescript": "^2.5.3",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "~3.8.1",
    "webpack-dev-server": "~2.9.3"
  }
}
